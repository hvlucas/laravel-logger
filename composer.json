{
    "name": "hvlucas/laravel-logger",
    "description": "Log Model events easily with laravel-logger! Package is NOT LIVE yet",
    "authors": [
        {
            "name": "Lucas Novoa",
            "email": "novoalg@hotmail.com",
            "homepage": "https://www.highervisibility.com#footer-consultation"
        }
    ],
    "license": "MIT",
    "type": "package",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "laravel"
    ],
    "require": {
        "php": ">=7.0.0",
        "datatables/datatables": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "HVLucas\\LaravelLogger\\": "src/"
        }
    },
    "scripts": {
        "post-package-install": [ 
            "@php artisan migrate"
        ],
        "post-package-update": [
            "@php artisan migrate"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HVLucas\\LaravelLogger\\LaravelLoggerServiceProvider",
                "HVLucas\\LaravelLogger\\LaravelLoggerRouteServiceProvider"
            ],
            "aliases": {
                "LaravelLogger": "HVLucas\\LaravelLogger\\Facades\\LaravelLogger"
            }
        }
    }
}
